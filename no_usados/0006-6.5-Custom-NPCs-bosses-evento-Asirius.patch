+class dragones_negros : public CreatureScript
+{
+public:
+    dragones_negros() : CreatureScript("dragones_negros") { }
+
+    CreatureAI* GetAI(Creature* pCreature) const
+    {
+        return new dragones_negrosAI(pCreature);
